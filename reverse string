public class reversestring {
    public static void main(String[] args) {
        String s = "hello";
        int len = s.length(); // Use length() method to get the string's length
        for (int i = len - 1; i >= 0; i--) { // Corrected to use len - 1
            System.out.println(s.charAt(i)); // Correct case for charAt and System
        }
        System.out.println();
    }
}
